<h1>新規投稿</h1>
<!--インスタンス変数(@list)を取得し、ブロック変数fに渡す。-->
<!--投稿ボタンを押すと、POSTメソッド(デフォルト)の'/todolists'に送信-->
<%= form_with model: @list, url:'/todolists', local:true do |f| %>
  <h4>タイトル</h4>
  <%= f.text_field :title %>
  <h4>本文</h4>
  <%= f.text_area :body %>
  <h4>画像</h4>
  <%= f.attachment_field :image %>
  <%= f.submit '投稿' %>
<% end %>